{
    "name": "runstack/obfuscator",
    "description": "RunStack Obfuscator é uma Ferramenta avançada de proteção de código PHP. Ofusca e dificulta engenharia reversa mantendo desempenho e compatibilidade. Ideal para distribuição de scripts comerciais, APIs privadas, plugins e software proprietário.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0-dev",
    "authors": [
        {
            "name": "Eduardo Costa Nkuansambu",
            "email": "eduardo@runstack.dev",
            "homepage": "https://runstack.dev",
            "role": "Lead Developer"
        }
    ],
    "keywords": [
        "obfuscator",
        "php-obfuscator",
        "code-protection",
        "security",
        "anti-reverse-engineering",
        "runstack"
    ],
    "homepage": "https://github.com/ecnmee/runstack-ops-obfuscator",
    "support": {
        "issues": "https://github.com/ecnmee/runstack-ops-obfuscator/issues",
        "source": "https://github.com/ecnmee/runstack-ops-obfuscator"
    },
    "require": {
        "php": ">=8.0",
        "nikic/php-parser": "^4.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RunStack\\Obfuscator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RunStack\\Obfuscator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}