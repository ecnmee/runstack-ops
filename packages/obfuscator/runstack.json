{
  "name": "runstack/obfuscator",
  "version": "1.0.0",
  "type": "runstack-package",
  "description": "Sistema de ofuscação PHP multi-nível para proteção de código",
  "keywords": [
    "obfuscator",
    "php",
    "code-protection",
    "encryption",
    "security"
  ],
  "homepage": "https://runstack.dev/obfuscator",
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo Costa Nkuansambu",
      "email": "eduardo@runstack.dev",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "runstack/core": "^1.0",
    "ext-openssl": "*",
    "nikic/php-parser": "^4.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "RunStack\\Obfuscator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RunStack\\Obfuscator\\Tests\\": "tests/"
    }
  },
  "versions": {
    "freemium": {
      "levels": [1, 2],
      "features": [
        "minify",
        "rename-variables",
        "rename-functions",
        "cli-basic"
      ],
      "price": 0,
      "github": true,
      "obfuscation": 0,
      "protection_rate": "30%"
    }
  },
  "provides": {
    "commands": [
      "obfuscate",
      "obfuscate:directory",
      "obfuscate:batch",
      "obfuscate:analyze"
    ],
    "api": {
      "endpoints": [
        "/api/obfuscate",
        "/api/obfuscate/batch",
        "/api/obfuscate/status/{id}"
      ],
      "version": "v1"
    }
  },
  "config": {
    "default_level": 1,
    "preserve_public": true,
    "strip_comments": true,
    "output_format": "php",
    "cache_enabled": true,
    "cache_path": "storage/obfuscator/cache"
  },
  "support": {
    "issues": "https://github.com/runstack/obfuscator/issues",
    "source": "https://github.com/runstack/obfuscator",
    "docs": "https://docs.runstack.dev/obfuscator",
    "email": "support@runstack.dev"
  }
}